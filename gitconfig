[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[alias]
	br = branch
	co = checkout
	ci = commit
	ss = status
	pick = cherry-pick
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%ci) %C(bold blue)<%an>%Creset:%ae' --abbrev-commit
[init]
	defaultBranch = main
[core]
	editor = vim
